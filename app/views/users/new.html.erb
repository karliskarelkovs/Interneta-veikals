<%= stylesheet_link_tag "login.css" %>
<%= form_for @user do |f| %>
<div id="wrapper">
  <table>
    <tr>
      <td><%= f.label :'E-pasts' %></td>
      <td><%= f.text_field :email %></td>
    </tr>
    <tr>
      <td><%= f.label :'Parole' %></td>
      <td><%= f.password_field :password %></td>
    </tr>
    <tr>
      <td><%= f.label :'Parole atkārtoti' %></td>
      <td><%= f.password_field :password_confirmation %></td>
    </tr>
    <tr>
      <td><%= f.submit 'Reģistrēties'%>
      <% end %>
      </td>
    </tr>
  </table>

</div>